{
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.scripts.tsbuildinfo",
        "target": "ES2022",
        "lib": [
            "ES2023"
        ],
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "rootDir": "./scripts",
        "outDir": "./dist/scripts",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true
    },
    "include": [
        "scripts"
    ]
}